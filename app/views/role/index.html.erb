<h1>Roles</h1>
<div>
  <ul>
    <% @roles.each do |role| %>
      <li>
        <%= link_to(role.name, role_path(role)) %>   <%= link_to("Edit", edit_role_path(role)) %>   <%= link_to("Delete", delete_role_path(role)) %>
      </li>
    <% end %>
  </ul>
  <%= link_to("Add Role", role_new_path) %>
</div>
