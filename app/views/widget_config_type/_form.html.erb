<%= form_with url: widget_config_types_path, model: @config_type do |f| %>
  <div>
    <%= f.label :name %><br>
    <%= f.text_field(:name, :value => @config_type.name) %>
  </div>
  <div>
    <%= f.label :label %><br>
    <%= f.text_field(:label, :value => @config_type.label) %>
  </div>
  <div>
    <%= f.label :partial_name %><br>
    <%= f.text_field(:partial_name, :value => @config_type.partial_name) %>
  </div>
  <div>
    <%= f.label :description %><br>
    <%= f.text_area(:description, :value => @config_type.description) %>
  </div>
  <div>
    <%= f.hidden_field :id, :value => @config_type.id %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
